/* pages/shop-manage/index.wxss */

/* 列表框 */
view, scroll-view,::after,::before {
    box-sizing: border-box;
    transition: .3s all;
}
.listBody {
    width: 100%;
    height: calc(100% - 48px);
    background: #fff;
    padding-left: 15px;
}

.listBody_li {
    border-bottom: 1px solid #f5f5f5;
    height: 50px;
    display: flex;
    justify-content: space-between;
}

.shopName {
    line-height: 50px;
    font-size: 14px;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.operation {
    width: 115px;
    height: 50px;
    margin-right: 15px;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.operationBtn {
    width: 50px;
    height: 24px;
    font-size: 12px;
    padding: 0 12px;
    line-height: 24px;
    background: #ffa20a;
    border-radius: 4px;
    color: #fff;
    text-align: center;
}

/* 字体设置 */

