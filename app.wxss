/**app.wxss**/

view, scroll-view,::after,::before {
    /* box-sizing: border-box; */
    transition: .3s all;
}
radio .wx-radio-input.wx-radio-input-checked{
    border-color:#FFA20A !important;
    background:#FFA20A !important;
}

.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    padding: 0;
    box-sizing: border-box;
}

.body {
    width: 100%;
    height: 100vh;
    background: #f5f5f5;
    padding: 10px;
    position: relative;
}

/* 搜索框 */

.seach {
    width: 100%;
    height: 38px;
    border-radius: 6px;
    background: #fff;
    margin-bottom: 10px;
}

.sousuoIcon {
    float: left;
    margin-left: 25px;
    line-height: 38px;
    color:#c6c6c6;
}

.seachInput {
    width: calc(100% - 60px);
    height: 100%;
    padding-left: 10px;
    line-height: 38px;
    font-size: 14px;
    float: left;
}

